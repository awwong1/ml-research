{
    "exp_name": "cifar100-alexnet",
    "agent": "agents.classification.ClassificationAgent",
    "cpu_only": false,
    "epochs": 164,
    "schedule": [
        81,
        122
    ],
    "gamma": 0.1,
    "model": {
        "name": "models.cifar.alexnet.alexnet",
        "kwargs": {
            "num_classes": 100
        }
    },
    "train_data": {
        "name": "torchvision.datasets.CIFAR100",
        "args": [
            "data/cifar100"
        ],
        "kwargs": {
            "train": true,
            "download": true
        },
        "transform": [
            {
                "name": "torchvision.transforms.RandomCrop",
                "args": [
                    32
                ],
                "kwargs": {
                    "padding": 4
                }
            },
            {
                "name": "torchvision.transforms.RandomHorizontalFlip"
            },
            {
                "name": "torchvision.transforms.ToTensor"
            },
            {
                "name": "torchvision.transforms.Normalize",
                "args": [
                    [
                        0.5073617,
                        0.4866903,
                        0.44108883
                    ],
                    [
                        0.26748818,
                        0.25659317,
                        0.27630863
                    ]
                ]
            }
        ],
        "dataloader_kwargs": {
            "batch_size": 128,
            "num_workers": 8,
            "shuffle": true
        }
    },
    "eval_data": {
        "name": "torchvision.datasets.CIFAR100",
        "args": [
            "data/cifar100"
        ],
        "kwargs": {
            "train": false,
            "download": true
        },
        "transform": [
            {
                "name": "torchvision.transforms.ToTensor"
            },
            {
                "name": "torchvision.transforms.Normalize",
                "args": [
                    [
                        0.5073617,
                        0.4866903,
                        0.44108883
                    ],
                    [
                        0.26748818,
                        0.25659317,
                        0.27630863
                    ]
                ]
            }
        ],
        "dataloader_kwargs": {
            "batch_size": 128,
            "num_workers": 8
        }
    },
    "task_loss": {
        "name": "torch.nn.CrossEntropyLoss"
    },
    "optimizer": {
        "name": "torch.optim.SGD",
        "kwargs": {
            "lr": 0.1,
            "momentum": 0.9,
            "weight_decay": 5e-4
        }
    }
}