{
    "exp_name": "imagenet-vgg19_bn",
    "agent": "agents.classification.ClassificationAgent",
    "cpu_only": false,
    "epochs": 164,
    "schedule": [81, 122],
    "gamma": 0.1,
    "model": {
        "name": "torchvision.models.vgg19_bn"
    },
    "train_data": {
        "name": "torchvision.datasets.ImageNet",
        "args": [
            "data/imagenet"
        ],
        "kwargs": {
            "split": "train",
            "download": true
        },
        "transform": [
            {
                "name": "torchvision.transforms.RandomResizedCrop",
                "args": [
                    224
                ]
            },
            {
                "name": "torchvision.transforms.RandomHorizontalFlip"
            },
            {
                "name": "torchvision.transforms.ToTensor"
            },
            {
                "name": "torchvision.transforms.Normalize",
                "args": [
                    [
                        0.485,
                        0.456,
                        0.406
                    ],
                    [
                        0.229,
                        0.224,
                        0.225
                    ]
                ]
            }
        ],
        "dataloader_kwargs": {
            "batch_size": 256,
            "num_workers": 8,
            "shuffle": true
        }
    },
    "eval_data": {
        "name": "torchvision.datasets.ImageNet",
        "args": [
            "data/imagenet"
        ],
        "kwargs": {
            "split": "val",
            "download": true
        },
        "transform": [
            {
                "name": "torchvision.transforms.Resize",
                "args": [256]
            },
            {
                "name": "torchvision.transforms.CenterCrop",
                "args": [224]
            },
            {
                "name": "torchvision.transforms.ToTensor"
            },
            {
                "name": "torchvision.transforms.Normalize",
                "args": [
                    [
                        0.485,
                        0.456,
                        0.406
                    ],
                    [
                        0.229,
                        0.224,
                        0.225
                    ]
                ]
            }
        ],
        "dataloader_kwargs": {
            "batch_size": 256,
            "num_workers": 8
        }
    },
    "task_loss": {
        "name": "torch.nn.CrossEntropyLoss"
    },
    "optimizer": {
        "name": "torch.optim.SGD",
        "kwargs": {
            "lr": 0.1,
            "momentum": 0.9,
            "weight_decay": 5e-4
        }
    }
}