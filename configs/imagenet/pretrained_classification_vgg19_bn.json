{
    "exp_name": "pretrained-imagenet-vgg19_bn",
    "agent": "agents.classification.ClassificationAgent",
    "cpu_only": false,
    "epochs": 0,
    "schedule": [],
    "gamma": 1,
    "force_store_checkpoint": true,
    "model": {
        "name": "torchvision.models.vgg19_bn",
        "kwargs": {
            "pretrained": true
        }

    },
    "train_data": {
        "name": "torchvision.datasets.ImageNet",
        "args": [
            "data/imagenet"
        ],
        "kwargs": {
            "split": "train",
            "download": true
        },
        "transform": [
            {
                "name": "torchvision.transforms.RandomResizedCrop",
                "args": [
                    224
                ]
            },
            {
                "name": "torchvision.transforms.RandomHorizontalFlip"
            },
            {
                "name": "torchvision.transforms.ToTensor"
            },
            {
                "name": "torchvision.transforms.Normalize",
                "args": [
                    [
                        0.48026853799819946,
                        0.457515150308609,
                        0.40817248821258545
                    ],
                    [
                        0.2807179391384125,
                        0.2736707925796509,
                        0.2878275215625763
                    ]
                ]
            }
        ],
        "dataloader_kwargs": {
            "batch_size": 256,
            "num_workers": 8,
            "shuffle": true
        }
    },
    "eval_data": {
        "name": "torchvision.datasets.ImageNet",
        "args": [
            "data/imagenet"
        ],
        "kwargs": {
            "split": "val",
            "download": true
        },
        "transform": [
            {
                "name": "torchvision.transforms.Resize",
                "args": [256]
            },
            {
                "name": "torchvision.transforms.CenterCrop",
                "args": [224]
            },
            {
                "name": "torchvision.transforms.ToTensor"
            },
            {
                "name": "torchvision.transforms.Normalize",
                "args": [
                    [
                        0.48026853799819946,
                        0.457515150308609,
                        0.40817248821258545
                    ],
                    [
                        0.2807179391384125,
                        0.2736707925796509,
                        0.2878275215625763
                    ]
                ]
            }
        ],
        "dataloader_kwargs": {
            "batch_size": 256,
            "num_workers": 8
        }
    },
    "task_loss": {
        "name": "torch.nn.CrossEntropyLoss"
    },
    "optimizer": {
        "name": "torch.optim.SGD",
        "kwargs": {
            "lr": 0.1,
            "momentum": 0.9,
            "weight_decay": 5e-4
        }
    }
}